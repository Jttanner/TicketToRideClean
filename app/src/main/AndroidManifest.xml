<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="teamjapannumbahone.tickettoride">
    <uses-permission android:gameName="android.permission.INTERNET"/>
    <uses-permission android:gameName="android.permission.ACCESS_NETWORK_STATE" />

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:gameName="ui.views.LoginActivity" android:screenOrientation="landscape">

            <intent-filter>
                <action android:gameName="android.intent.action.MAIN" />

                <category android:gameName="android.intent.category.LAUNCHER" />

            </intent-filter>

        </activity>

        <activity android:gameName="ui.views.GameListActivity" android:screenOrientation="landscape">
            android:screenOrientation="landscape"


        </activity>

    </application>



</manifest>